// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Resource {
  id        Int      @id @default(autoincrement())
  name      String
  type      String
  url       String
  createdAt DateTime @default(now())
}
