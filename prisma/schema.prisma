generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Resource {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  tags        String?
  sort_order  Int      @default(0)
  created_at  DateTime @default(now())
}
